CORR=function(n){
x=runif(n,0,100)
y=NULL
for (i in 1:n){
a=exp(-2+0.04*x[i])
p=a/(1+a)
y=cbind(y,rbinom(1,1,p))
}
fm=glm(y[1,]~x, family=binomial(link=logit))
pre=predict(fm, data.frame(x=x))
u=exp(pre)/(1+exp(pre))
cor(y[1,]-u,u)
}
CORR(25)
CORR(100)
CORR(1000)
CORR(10000)
